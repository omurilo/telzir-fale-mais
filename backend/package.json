{
  "name": "falemais",
  "version": "1.0.0",
  "description": "Fale Mais - Telzir",
  "main": "src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development jest --detectOpenHandles",
    "test:prod": "cross-env NODE_ENV=production npm run test",
    "coverage": "nyc npm run test",
    "coverage:prod": "nyc npm run test:prod",
    "prod": "cross-env NODE_ENV=production npm run build && pm2-runtime dist/index.js",
    "start": "pm2 start babel-node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src -d dist",
    "docker:dev": "docker-compose --env-file .env.development up -d"
  },
  "author": "Murilo Henrique",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^20.0.0",
    "@hapi/inert": "^6.0.2",
    "@hapi/vision": "^6.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "hapi-sentry": "^3.1.0",
    "hapi-swagger": "^14.0.0",
    "joi": "^17.2.1",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.4.1",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "sequelize-cli": "^6.2.0"
  }
}
